{
  "name": "claude-library",
  "version": "2.0.0",
  "description": "Reusable component library for Claude Code projects",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:py": "python -m pytest",
    "test:py:strict": "set PYTHONPATH=. && python -W error -m pytest",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint components --ext .ts,.tsx"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@tanstack/react-query": "^5.90.20",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react-router-dom": "^7.13.0",
    "typescript": "^5.3.0",
    "vitest": "^2.1.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-dropdown-menu": "^2.0.0",
    "express": "^4.18.0",
    "ioredis": "^5.3.0",
    "jose": "^5.2.0",
    "stripe": "^14.0.0",
    "zod": "^3.22.0",
    "zustand": "^4.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
